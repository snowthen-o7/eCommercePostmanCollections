		{
			"name": "Products",
			"item": [
				{
					"name": "List Products",
					"request": {
						"method": "GET",
						"header": [
							{"key": "Authorization", "value": "Bearer {{magento_token}}", "type": "text"},
							{"key": "Content-Type", "value": "application/json", "type": "text"}
						],
						"url": {
							"raw": "{{magento_store_url}}/rest/V1/products?searchCriteria[currentPage]=1&searchCriteria[pageSize]=10",
							"host": ["{{magento_store_url}}"],
							"path": ["rest", "V1", "products"],
							"query": [
								{"key": "searchCriteria[currentPage]", "value": "1"},
								{"key": "searchCriteria[pageSize]", "value": "10"}
							]
						},
						"description": "Retrieves a paginated list of products from the catalog"
					},
					"event": [
						{
							"listen": "test",
							"script": {
								"type": "text/javascript",
								"exec": [
									"pm.test('Status code is 200', function() { pm.response.to.have.status(200); });",
									"pm.test('Response is JSON', function() { pm.response.to.be.json; });",
									"try {",
									"    const response = pm.response.json();",
									"    if (response.items && response.items.length > 0) {",
									"        pm.environment.set('magento_product_sku', response.items[0].sku);",
									"        console.log('Extracted product_sku:', response.items[0].sku);",
									"    }",
									"} catch(e) { }"
								]
							}
						}
					]
				},
				{
					"name": "Get Product by SKU",
					"request": {
						"method": "GET",
						"header": [
							{"key": "Authorization", "value": "Bearer {{magento_token}}", "type": "text"},
