					},
					"event": [
						{
							"listen": "test",
							"script": {
								"type": "text/javascript",
								"exec": [
									"pm.test('Status code is 200', function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test('Response time is acceptable', function () {",
									"    pm.expect(pm.response.responseTime).to.be.below(5000);",
									"});",
									"",
									"pm.test('Response is valid JSON', function () {",
									"    pm.response.to.be.json;",
									"});",
									"",
									"// Extract first product ID for subsequent tests",
									"try {",
									"    const response = pm.response.json();",
									"    if (Array.isArray(response) && response.length > 0) {",
									"        const firstProduct = response[0];",
									"        pm.environment.set('woo_product_id', firstProduct.id);",
									"        console.log('Extracted product_id:', firstProduct.id);",
									"        ",
									"        // Check for variable product to get variations",
									"        const variableProduct = response.find(p => p.type === 'variable');",
									"        if (variableProduct) {",
									"            pm.environment.set('woo_variable_product_id', variableProduct.id);",
									"            console.log('Extracted variable_product_id:', variableProduct.id);",
									"        }",
									"    }",
									"} catch(e) { console.log('Error extracting IDs:', e); }"
								]
							}
						}
					]
				},
